{
  "providers": {
    "google": {
      // Required
      "apiKey": "${GOOGLE_API_KEY}",
      "model": "gemini-1.5-pro",

      // Optional
      "timeout": 30000,

      // Model-specific options
      "defaultOptions": {
        "temperature": 0.7,
        "maxOutputTokens": 2048,
        "topK": 40,
        "topP": 0.95
      }
    }
  }
}